    vs:term_status "testing" .

foaf:pastProject
    a rdf:Property ;
    rdfs:comment "A project this person has previously worked on." ;
    rdfs:domain foaf:Person ;
    rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
    rdfs:label "past project" ;
    rdfs:range rdfs:Resource ;
    vs:term_status "testing" .

foaf:phone
    a rdf:Property ;
