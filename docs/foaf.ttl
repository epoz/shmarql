    rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
    rdfs:label "image" ;
    rdfs:range foaf:Image ;
    rdfs:subPropertyOf foaf:depiction ;
    vs:term_status "testing" .

foaf:interest
    a rdf:Property ;
    rdfs:comment "A page about a topic of interest to this person." ;
